{
    "version": "4.1.1-rc1",
    "description": "A feature-packed, cross-platform game engine to create 2D and 3D games from a unified interface.",
    "homepage": "https://godotengine.org/",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://downloads.tuxfamily.org/godotengine/4.1.1/rc1/mono/Godot_v4.1.1-rc1_mono_win64.zip",
            "hash": "sha512:7505c8513b2b71a3dc380c7a809076d59d190903ff36479ff3deca749a7dc13ed653751737c1539b49d76f16fc07fcff922c76e2af5ec7137c323bd53c93efde",
            "extract_dir": "Godot_v4.1.1-rc1_mono_win64"
        },
        "32bit": {
            "url": "https://downloads.tuxfamily.org/godotengine/4.1.1/rc1/mono/Godot_v4.1.1-rc1_mono_win32.zip",
            "hash": "sha512:3abb3414bf338d268e0a2ab416341caa7aa30ae736dca5f220458995d650aee56ac849c80cf6952df5226284080251772593b5a1730b8f6d4c728f9fedc84b86",
            "extract_dir": "Godot_v4.1.1-rc1_mono_win32"
        }
    },
    "pre_install": [
        "Remove-Item \"$dir\\Godot_*_console.*\"",
        "Get-Item \"$dir\\Godot_*.exe\" | Rename-Item -NewName 'godot-mono.exe'"
    ],
    "bin": "godot-mono.exe",
    "shortcuts": [
        [
            "godot-mono.exe",
            "Godot Engine (Mono)"
        ]
    ],
    "checkver": {
        "url": "https://godotengine.org/blog/pre-release/",
        "regex": "Release candidate: Godot (?<ver>[\\d.]+) RC (?<rc>[\\d.]+)",
        "replace": "${1}-rc${2}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://downloads.tuxfamily.org/godotengine/$matchVer/$preReleaseVersion/mono/Godot_v$version_mono_win64.zip",
                "extract_dir": "Godot_v$version_mono_win64"
            },
            "32bit": {
                "url": "https://downloads.tuxfamily.org/godotengine/$matchVer/$preReleaseVersion/mono/Godot_v$version_mono_win32.zip",
                "extract_dir": "Godot_v$version_mono_win32"
            }
        },
        "hash": {
            "url": "$baseurl/SHA512-SUMS.txt"
        }
    }
}
