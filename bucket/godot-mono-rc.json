{
    "version": "4.0.1-rc1",
    "description": "A feature-packed, cross-platform game engine to create 2D and 3D games from a unified interface.",
    "homepage": "https://godotengine.org/",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://downloads.tuxfamily.org/godotengine/4.0.1/rc1/mono/Godot_v4.0.1-rc1_mono_win64.zip",
            "hash": "sha512:827ff25bf51ebcc78eb4e1b84b2b83ca21030171fb1ce6e84e8c082b1ebf6026e345e87f69c338943431365d7213da6726af43852db82fc70ef386b6086562d2",
            "extract_dir": "Godot_v4.0.1-rc1_mono_win64"
        },
        "32bit": {
            "url": "https://downloads.tuxfamily.org/godotengine/4.0.1/rc1/mono/Godot_v4.0.1-rc1_mono_win32.zip",
            "hash": "sha512:d906a3f9f520e2c8ab8173df09d19640274de93b7a3a9ec271ba4a5d58804ced93c93edb673fe3c64acd257e113cb3a5595c05f93c218e5de0678b1b6723c50b",
            "extract_dir": "Godot_v4.0.1-rc1_mono_win32"
        }
    },
    "pre_install": [
        "Remove-Item \"$dir\\Godot_*_console.*\"",
        "Get-Item \"$dir\\Godot_*.exe\" | Rename-Item -NewName 'godot-mono.exe'"
    ],
    "bin": "godot-mono.exe",
    "shortcuts": [
        [
            "godot-mono.exe",
            "Godot Engine (Mono)"
        ]
    ],
    "checkver": {
        "url": "https://godotengine.org/blog/pre-release/",
        "regex": "Release candidate: Godot (?<ver>[\\d.]+) RC (?<rc>[\\d.]+)",
        "replace": "${1}-rc${2}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://downloads.tuxfamily.org/godotengine/$matchVer/$preReleaseVersion/mono/Godot_v$version_mono_win64.zip",
                "extract_dir": "Godot_v$version_mono_win64"
            },
            "32bit": {
                "url": "https://downloads.tuxfamily.org/godotengine/$matchVer/$preReleaseVersion/mono/Godot_v$version_mono_win32.zip",
                "extract_dir": "Godot_v$version_mono_win32"
            }
        },
        "hash": {
            "url": "$baseurl/SHA512-SUMS.txt"
        }
    }
}
