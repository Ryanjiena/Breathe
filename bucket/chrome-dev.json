        "    ensure \"$dir\\User Data\" | Out-Null",
        "    Copy-Item \"$env:LocalAppData\\Google\\Chrome\\User Data\\*\" \"$dir\\User Data\" -Recurse",
        "}"
    ],
    "persist": "User Data",
    "checkver": {
        "url": "https://github.com/Ryanjiena/MetaVersion/raw/main/chrome.json",
