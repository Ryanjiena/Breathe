{
    "version": "3.0.2",
    "description": "Flutter Version Management: A simple CLI to manage Flutter SDK versions.",
    "homepage": "https://fvm.app/",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/fluttertools/fvm/releases/download/3.0.2/fvm-3.0.2-windows-x64.zip",
            "hash": "da6b761c1212b5a9d8c9459393bf588dbbb8c898193433cf44cbcf7be810c7bb"
        },
        "32bit": {
            "url": "https://github.com/fluttertools/fvm/releases/download/3.0.2/fvm-3.0.2-windows-ia32.zip",
            "hash": "3a9d33c97378f7e005d6f2edb3caa27e0067276f131a754a5e08ba0a8ebb5e7d"
        }
    },
    "extract_dir": "fvm",
    "pre_install": [
        "if (!(Test-Path \"$persist_dir\\versions\")) {",
        "   New-Item \"$dir\\versions\" -ItemType Directory | Out-Null",
        "   New-Item \"$dir\\.settings\" -ItemType File | Out-Null",
        "}"
    ],
    "bin": "fvm.bat",
    "env_set": {
        "FVM_HOME": "$dir"
    },
    "persist": [
        "versions",
        ".settings"
    ],
    "checkver": {
        "github": "https://github.com/fluttertools/fvm"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/fluttertools/fvm/releases/download/$version/fvm-$version-windows-x64.zip"
            },
            "32bit": {
                "url": "https://github.com/fluttertools/fvm/releases/download/$version/fvm-$version-windows-ia32.zip"
            }
        }
    }
}
