{
    "version": "3.1.3",
    "description": "Flutter Version Management: A simple CLI to manage Flutter SDK versions.",
    "homepage": "https://fvm.app/",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/fluttertools/fvm/releases/download/3.1.3/fvm-3.1.3-windows-x64.zip",
            "hash": "f9467426b578efb5a7908de7d47826fc49bdd25e4194a303fcbc877b693ab367"
        },
        "32bit": {
            "url": "https://github.com/fluttertools/fvm/releases/download/3.1.3/fvm-3.1.3-windows-ia32.zip",
            "hash": "c7c6e8309fd81c48bb3c7bc67563f95b11bf4ac66ddfdeea106bd14ec13b8934"
        }
    },
    "extract_dir": "fvm",
    "pre_install": [
        "if (!(Test-Path \"$persist_dir\\versions\")) {",
        "   New-Item \"$dir\\versions\" -ItemType Directory | Out-Null",
        "   New-Item \"$dir\\.settings\" -ItemType File | Out-Null",
        "}"
    ],
    "bin": "fvm.bat",
    "env_set": {
        "FVM_HOME": "$dir"
    },
    "persist": [
        "versions",
        ".settings"
    ],
    "checkver": {
        "github": "https://github.com/fluttertools/fvm"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/fluttertools/fvm/releases/download/$version/fvm-$version-windows-x64.zip"
            },
            "32bit": {
                "url": "https://github.com/fluttertools/fvm/releases/download/$version/fvm-$version-windows-ia32.zip"
            }
        }
    }
}
