{
    "version": "3.1.0",
    "description": "Flutter Version Management: A simple CLI to manage Flutter SDK versions.",
    "homepage": "https://fvm.app/",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://github.com/fluttertools/fvm/releases/download/3.1.0/fvm-3.1.0-windows-x64.zip",
            "hash": "93886522b607af4dcab2fddc67e317ff65ec11e0b91d3575b7318f889e11631f"
        },
        "32bit": {
            "url": "https://github.com/fluttertools/fvm/releases/download/3.1.0/fvm-3.1.0-windows-ia32.zip",
            "hash": "6d1d62e0d1521548c25e5fcf2d99555eac5f0ff49bb7a92c020d50fe26c4db41"
        }
    },
    "extract_dir": "fvm",
    "pre_install": [
        "if (!(Test-Path \"$persist_dir\\versions\")) {",
        "   New-Item \"$dir\\versions\" -ItemType Directory | Out-Null",
        "   New-Item \"$dir\\.settings\" -ItemType File | Out-Null",
        "}"
    ],
    "bin": "fvm.bat",
    "env_set": {
        "FVM_HOME": "$dir"
    },
    "persist": [
        "versions",
        ".settings"
    ],
    "checkver": {
        "github": "https://github.com/fluttertools/fvm"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/fluttertools/fvm/releases/download/$version/fvm-$version-windows-x64.zip"
            },
            "32bit": {
                "url": "https://github.com/fluttertools/fvm/releases/download/$version/fvm-$version-windows-ia32.zip"
            }
        }
    }
}
