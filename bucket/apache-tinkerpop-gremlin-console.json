{
    "version": "3.6.3",
    "description": "A REPL environment for interactive development and analysis of Gremlin applications.",
    "homepage": "https://tinkerpop.apache.org",
    "license": "Apache-2.0",
    "url": "https://www.apache.org/dist/tinkerpop/3.6.3/apache-tinkerpop-gremlin-console-3.6.3-bin.zip",
    "hash": "sha512:2309c98831dc9f1b39921f46cd9d3163b13046fabbb7e73efb903bc3d3fd51cedec6fbe2d91efd490dc788185306d7aad332ce4cdc3007ed2077499677f021e3",
    "extract_dir": "apache-tinkerpop-gremlin-console-3.6.3",
    "pre_install": [
        "$cont = @(",
        "    '@echo off'",
        "    'cd /d \"%~dp0\"'",
        "    'gremlin.bat'",
        ")",
        "Set-Content \"$dir\\bin\\gremlin-scoop.bat\" $cont -Encoding ascii"
    ],
    "bin": [
        [
            "bin\\gremlin-scoop.bat",
            "gremlin"
        ]
    ],
    "checkver": {
        "url": "https://www.apache.org/dist/tinkerpop/",
        "regex": "href=\"([\\d.]+)/",
        "reverse": true
    },
    "autoupdate": {
        "url": "https://www.apache.org/dist/tinkerpop/$version/apache-tinkerpop-gremlin-console-$version-bin.zip",
        "hash": {
            "url": "$url.sha512"
        },
        "extract_dir": "apache-tinkerpop-gremlin-console-$version"
    }
}
