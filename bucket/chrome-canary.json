            "hash": "b51530911faa78633f225ce4b8fac8dff18563be30a47cc28fc3e3e39f8bf2f4"
        },
        "32bit": {
            "url": "https://dl.google.com/release2/chrome/adeu4l5edojdellkj3l7bp5l5jra_105.0.5195.3/105.0.5195.3_chrome_installer.exe#/dl.7z",
