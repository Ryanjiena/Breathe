{
    "version": "4.0-alpha13",
    "description": "A feature-packed, cross-platform game engine to create 2D and 3D games from a unified interface.",
    "homepage": "https://godotengine.org/",
    "license": "MIT",
    "architecture": {
        "64bit": {
            "url": "https://downloads.tuxfamily.org/godotengine/4.0/alpha13/Godot_v4.0-alpha13_win64.exe.zip",
            "hash": "04e2ea15bbbc79d3e392521010c1404839325c5b77a9af26ff9e41eaff77cda9"
        },
        "32bit": {
            "url": "https://downloads.tuxfamily.org/godotengine/4.0/alpha13/Godot_v4.0-alpha13_win32.exe.zip",
            "hash": "10319023afb37277619c33d482ef48356d36e559abb9bc48a003dac87f0e87a6"
        }
    },
    "pre_install": "Get-ChildItem \"$dir\\Godot_*.exe\" | Rename-Item -NewName \"$dir\\godot.exe\"",
    "bin": "godot.exe",
    "shortcuts": [
        [
            "godot.exe",
            "Godot Engine"
        ]
    ],
    "checkver": {
        "url": "https://godotengine.org/news",
        "regex": "Dev snapshot: Godot (?<ver>[\\d.]+) alpha (?<alpha>[\\d.]+)",
        "replace": "${1}-alpha${2}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://downloads.tuxfamily.org/godotengine/$matchVer/alpha$matchAlpha/Godot_v$version_win64.exe.zip"
            },
            "32bit": {
                "url": "https://downloads.tuxfamily.org/godotengine/$matchVer/alpha$matchAlpha/Godot_v$version_win32.exe.zip"
            }
        }
    }
}
