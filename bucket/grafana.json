        [
            "bin\\grafana-server.exe",
            "grafana-server",
            "-homepath \"$dir\""
        ]
    ],
    "persist": [
        "conf",
        "data"
    ],
    "checkver": {
        "url": "https://grafana.com/grafana/download?platform=windows",
