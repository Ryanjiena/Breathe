{
    "homepage": "https://github.com/GitTools/GitVersion",
    "description": "Easy Semantic Versioning for projects using Git.",
    "license": "MIT",
    "version": "6.0.0-rc-2",
    "url": "https://packages.chocolatey.org/GitVersion.Portable.6.0.0-rc-2.nupkg",
    "hash": "956078831bdec045018a9ea757e94847756b4ebdc3508e5a5986e7328866d476",
    "extract_dir": "tools",
    "bin": "GitVersion.exe",
    "checkver": {
        "url": "https://chocolatey.org/packages/GitVersion.Portable",
        "regex": "GitVersion ([\\w.-]+)"
