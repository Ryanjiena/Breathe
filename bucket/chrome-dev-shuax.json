    "license": {
        "identifier": "Freeware",
        "url": "https://www.google.com/chrome/privacy/eula_text.html"
    },
    "depends": "chrome-plus",
    "architecture": {
        "64bit": {
            "url": "https://dl.google.com/release2/chrome/adoyucrmwcc7aoyqizwv3ojenawa_108.0.5355.0/108.0.5355.0_chrome_installer.exe#/dl.7z",
            "hash": "fa00d1387d677da12c519c2104bb9ff85ff22f70940fd5f0ec308b6111b63b0b"
        },
        "32bit": {
            "url": "https://dl.google.com/release2/chrome/ni7v2m55zd7mdufsxxpvizcaza_108.0.5355.0/108.0.5355.0_chrome_installer.exe#/dl.7z",
            "hash": "2b8e5ef8d4ad77658799e2972c68b71bb99694c0497179d83ff146b5e0f89e2c"
        }
    },
    "installer": {
        "script": [
            "Expand-7zipArchive \"$dir\\chrome.7z\" -ExtractDir 'Chrome-bin' -Removal",
            "New-Item -Path \"$dir\" -Name \"App\" -ItemType \"directory\" | Out-Null",
            "Get-ChildItem \"$dir\" -Exclude *install.json, *manifest.json, App | ForEach-Object { Move-Item $_.FullName \"$dir\\App\\\" -Force -ErrorAction SilentlyContinue }",
            "#Get-ChildItem \"$(appdir chrome-plus $global)\\current\\\" -Exclude *install.json, *manifest.json | ForEach-Object { Copy-Item $_.FullName \"$dir\\\" -Recurse -Force -ErrorAction SilentlyContinue }",
            "Get-ChildItem \"$(appdir chrome-plus $global)\\current\\\" -Exclude *install.json, *manifest.json | ForEach-Object { Copy-Item $_.FullName \"$dir\\App\\\" -Recurse -Force -ErrorAction SilentlyContinue }",
            "@('Chrome.exe', '说明.txt', '清理.bat') | ForEach-Object { Copy-Item \"$bucketsdir\\tian\\scripts\\chrome\\$_\" \"$dir\\$_\" -Force -ErrorAction 'SilentlyContinue' | Out-Null }"
        ]
    },
    "pre_uninstall": "Start-Process \"$dir\\清理.bat\" -WorkingDirectory \"$dir\" -Wait",
    "bin": [
        [
            "Chrome.exe",
            "chrome-dev-plus"
        ]
    ],
    "shortcuts": [
        [
            "Chrome.exe",
            "Google Chrome Dev Plus"
        ]
    ],
    "persist": [
        "Data",
        "Cache"
    ],
    "checkver": {
        "url": "https://github.com/Ryanjiena/MetaVersion/raw/main/chrome.json",
        "regex": "(?sm)dev.*?win-x86.*?(?<86>chrome/[\\w-]+_(?<version>[\\d.]+).*?_chrome_installer.exe).*?win-x64.*?(?<64>chrome/[\\w-]+_(?<version>[\\d.]+).*?_chrome_installer.exe)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://dl.google.com/release2/$match64#/dl.7z",
                "hash": {
                    "url": "https://github.com/Ryanjiena/MetaVersion/raw/main/chrome.json",
                    "jsonpath": "$[?(@.Product == 'dev')].Release[?(@.Architecture == 'win-x64')].Hash"
                }
            },
            "32bit": {
                "url": "https://dl.google.com/release2/$match86#/dl.7z",
                "hash": {
                    "url": "https://github.com/Ryanjiena/MetaVersion/raw/main/chrome.json",
                    "jsonpath": "$[?(@.Product == 'dev')].Release[?(@.Architecture == 'win-x86')].Hash"
                }
            }
        }
    }
}
