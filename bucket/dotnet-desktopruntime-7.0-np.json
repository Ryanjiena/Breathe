    },
    "notes": "This package only contains the .NET 7 desktop runtime, install 'dotnet-sdk' instead if you need .NET Core SDK",
    "depends": "dark",
    "architecture": {
        "64bit": {
            "url": "https://dotnetcli.blob.core.windows.net/dotnet/WindowsDesktop/7.0.0-preview.7.22377.1/windowsdesktop-runtime-7.0.0-preview.7.22377.1-win-x64.exe",
            "hash": "sha512:4135ee9c6546c72b90054c5deabbaa824c32fb832d71b9c7b73b146a4e2150e859331ae40ea2ed8f13e9d18dc9c0fb35aff0815beb1af2aa0833b8ebdac9ccb0"
        },
        "32bit": {
            "url": "https://dotnetcli.blob.core.windows.net/dotnet/WindowsDesktop/7.0.0-preview.7.22377.1/windowsdesktop-runtime-7.0.0-preview.7.22377.1-win-x86.exe",
            "hash": "sha512:b89f66cdde6d97cc3fe78d6223414fc2d75b7caca67b98c48d8adf5c45c3f2c40ce50833849740aac1902ca3eeb59183e04abad81e2e5d7beaf4e80f89335740"
        }
    },
    "installer": {
        "script": [
            "Expand-DarkArchive \"$dir\\$fname\" \"$dir\\extracted\" -Removal",
            "Get-ChildItem \"$dir\\extracted\\AttachedContainer\\*.msi\" | ForEach-Object { Expand-MsiArchive $_ \"$dir\" }",
            "Move-Item \"$dir\\dotnet\\*\" \"$dir\" -Force",
            "@('dotnet', 'extracted') | ForEach-Object { Remove-Item \"$dir\\$_\" -Recurse }"
        ]
    },
    "bin": "dotnet.exe",
    "env_set": {
        "DOTNET_ROOT": "$dir"
    },
    "checkver": {
        "url": "https://dotnetcli.blob.core.windows.net/dotnet/release-metadata/7.0/releases.json",
        "jsonpath": "$.releases[0].windowsdesktop.version"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://dotnetcli.blob.core.windows.net/dotnet/WindowsDesktop/$version/windowsdesktop-runtime-$version-win-x64.exe",
                "hash": {
                    "url": "https://dotnetcli.blob.core.windows.net/dotnet/release-metadata/7.0/releases.json",
                    "jsonpath": "$.releases[0].windowsdesktop.files[?(@.name == 'windowsdesktop-runtime-win-x64.exe')].hash"
                }
            },
            "32bit": {
                "url": "https://dotnetcli.blob.core.windows.net/dotnet/WindowsDesktop/$version/windowsdesktop-runtime-$version-win-x86.exe",
                "hash": {
                    "url": "https://dotnetcli.blob.core.windows.net/dotnet/release-metadata/7.0/releases.json",
                    "jsonpath": "$.releases[0].windowsdesktop.files[?(@.name == 'windowsdesktop-runtime-win-x86.exe')].hash"
                }
            }
        }
    }
}
