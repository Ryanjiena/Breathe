{
    "description": "Official production-ready open-source builds of OpenJDK 20",
    "homepage": "https://jdk.java.net/20",
    "version": "20-34-ea",
    "license": "GPL-2.0-only WITH Classpath-exception-2.0",
    "architecture": {
        "64bit": {
            "url": "https://download.java.net/java/early_access/jdk20/34/GPL/openjdk-20-ea+34_windows-x64_bin.zip",
            "hash": "0387c564202f0ee1dc22d6316c79857a048210bf5d5de93fa23be9c88845b9a3"
        }
    },
    "extract_dir": "jdk-20",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "url": "https://jdk.java.net/20",
        "re": "/(?<type>early_access|GA)/(?<path>jdk(?<major>[\\d.]+)(?:.*)?/(?<build>[\\d]+)(?:/GPL|/binaries)?)/(?<file>openjdk-(?<version>[\\d.]+)(?<ea>-ea)?(?:\\+[\\d]+)?_windows-x64_bin.(zip|tar.gz))",
        "replace": "${version}-${build}${ea}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.java.net/java/$matchType/$matchPath/$matchFile"
            }
        },
        "hash": {
            "url": "$url.sha256"
        },
        "extract_dir": "jdk-$matchVersion"
    }
}
