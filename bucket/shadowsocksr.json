    ],
    "bin": "ShadowsocksR.exe",
    "shortcuts": [
        [
            "ShadowsocksR.exe",
            "ShadowsocksR"
        ]
    ],
    "persist": [
        "gui-config.json",
        "pac.txt",
        "user-rule.txt"
    ],
    "checkver": {
        "url": "https://github.com/HMBSbige/ShadowsocksR-Windows/releases",
        "regex": "<span class.*?ShadowsocksR-([\\d.]+)\\.7z</span>"
    },
    "autoupdate": {
