    },
    "extract_dir": "AdGuardHome",
    "pre_install": [
        "if (Test-Path \"$persist_dir\\AdGuardHome.yaml\") {",
        "    Copy-Item \"$persist_dir\\AdGuardHome.yaml\" \"$dir\\\" | Out-Null",
        "}"
    ],
    "uninstaller": {
        "script": [
            "if (Test-Path \"$dir\\AdGuardHome.yaml\") {",
