    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://mirrors.sdu.edu.cn/github-release/agalwood_Motrix/v$version/Motrix-Setup-$version.exe#/dl.7z",
                "hash": {
                    "url": "$baseurl/latest.yml",
                    "regex": "sha512:\\s+$base64"
                }
            }
        }
    }
}
