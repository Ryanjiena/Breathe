    "license": {
        "identifier": "AGPL-3.0-or-later",
        "url": "https://github.com/ciderapp/Cider/blob/main/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/ciderapp/cider-releases/releases/download/v1.5.1-beta.285/Cider-Setup-1.5.1-beta.285.exe#/dl.7z",
            "hash": "sha512:c3f9484e3f708734e5683afd2db36a8c8d61caa3cdcb0c44db85e2d8caa4be371ded9c850fedf7531751dd67329f9b4ef343218de5e12686e494fdad2b11b346",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\" -Removal"
        }
    },
    "post_install": "Remove-Item \"$dir\\`$PLUGINSDIR\", \"$dir\\`$R0\" -Recurse -Force",
    "shortcuts": [
        [
            "Cider.exe",
            "Cider",
            "--user-data-dir=\"$dir\\User Data\""
