        "identifier": "AGPL-3.0-or-later",
        "url": "https://github.com/ciderapp/Cider/blob/main/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/ciderapp/cider-releases/releases/download/v1.5.1-beta.347/Cider-Setup-1.5.1-beta.347.exe#/dl.7z",
            "hash": "sha512:616e2b57672231553bf75674b6e70855c9565ee910d5253fbd0f8df98dee0cfbc2a0c1cb2af1db93648e618259cf5efd45f3ba8df337a768adc87b0351b322eb",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\" -Removal"
        }
    },
    "post_install": "Remove-Item \"$dir\\`$PLUGINSDIR\", \"$dir\\`$R0\" -Recurse -Force",
