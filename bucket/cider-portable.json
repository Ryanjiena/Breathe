    "license": {
        "identifier": "AGPL-3.0-or-later",
        "url": "https://github.com/ciderapp/Cider/blob/main/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/ciderapp/cider-releases/releases/download/v1.5.1-beta.335/Cider-Setup-1.5.1-beta.335.exe#/dl.7z",
            "hash": "sha512:0782baee0c92b90e4df4920235332dcd4ccc6b59c1daa74248c812cd12b1e043980d028a023815fde06d125de697f304ff66240aec95d045f7980838dfc8a320",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\" -Removal"
        }
    },
    "post_install": "Remove-Item \"$dir\\`$PLUGINSDIR\", \"$dir\\`$R0\" -Recurse -Force",
