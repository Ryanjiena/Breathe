    "license": {
        "identifier": "AGPL-3.0-or-later",
        "url": "https://github.com/ciderapp/Cider/blob/main/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/ciderapp/cider-releases/releases/download/v1.5.4-beta.47/Cider-Setup-1.5.4-beta.47.exe#/dl.7z",
            "hash": "sha512:ea7b2ed73b11f4fd3a9327e42c1c3d5f4469da2d95f91dfb3e5bceb3a91d447d0b0819ed8a89da049bc791dd021d26a47e7d19ce27114bcad41cb7a17c72b251",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\" -Removal"
        }
    },
    "post_install": "Remove-Item \"$dir\\`$PLUGINSDIR\", \"$dir\\`$R0\" -Recurse -Force",
    "shortcuts": [
        [
            "Cider.exe",
            "Cider",
            "--user-data-dir=\"$dir\\User Data\""
