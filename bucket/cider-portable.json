    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/ciderapp/cider-releases/releases/download/v1.5.6-beta.35/Cider-Setup-1.5.6-beta.35.exe#/dl.7z",
            "hash": "sha512:a8e2528693d8182e0d60da3066e6d4b028068bfce3ae99d3cad360bc63a86952f58d2b094ac479dfb862c2eba41eea9759a8f9b62e1c0983207e1da864ead0d0",
