    "homepage": "https://cider.sh",
    "license": {
        "identifier": "AGPL-3.0-or-later",
        "url": "https://github.com/ciderapp/Cider/blob/main/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/ciderapp/cider-releases/releases/download/v1.5.5-beta.114/Cider-Setup-1.5.5-beta.114.exe#/dl.7z",
            "hash": "sha512:247c208c918985ec259634917726b3c297d87188fcc84859bfe66e13a4b6a35c5f45850d349944b48f892e8efa18b5e9f0693773b170f70722a19a190758ef29",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\" -Removal"
        }
    },
    "post_install": "Remove-Item \"$dir\\`$PLUGINSDIR\", \"$dir\\`$R0\" -Recurse -Force",
    "shortcuts": [
        [
            "Cider.exe",
