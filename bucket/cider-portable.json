        "regex": "v(.*)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/ciderapp/cider-releases/releases/download/v$version/Cider-Setup-$version.exe#/dl.7z",
                "hash": {
                    "url": "$baseurl/latest.yml",
                    "regex": "(?sm)$basename.*?sha512:\\s+$base64"
                }
            }
        }
    }
