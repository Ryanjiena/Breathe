    "license": {
        "identifier": "AGPL-3.0-or-later",
        "url": "https://github.com/ciderapp/Cider/blob/main/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/ciderapp/cider-releases/releases/download/v1.5.4-beta.78/Cider-Setup-1.5.4-beta.78.exe#/dl.7z",
            "hash": "sha512:44c70553da05f9e02e01f922bb51472c1557434cfd145013ae06d3ddcb1968f471b2d72a2f6b2077bd6e3a72104a57103fb838e43384d3362e4d79faa06924ac",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\" -Removal"
        }
    },
    "post_install": "Remove-Item \"$dir\\`$PLUGINSDIR\", \"$dir\\`$R0\" -Recurse -Force",
