        "identifier": "AGPL-3.0-or-later",
        "url": "https://github.com/ciderapp/Cider/blob/main/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/ciderapp/cider-releases/releases/download/v1.5.4-beta.38/Cider-Setup-1.5.4-beta.38.exe#/dl.7z",
            "hash": "sha512:50ef5efd78482ccb2c2b7d82f35863bc0c6b7d3e10c1b842ae539f8583c87966e5be859961036d63c58d9223bba5f697868400500bc335414b59304f1482262e",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\" -Removal"
        }
    },
