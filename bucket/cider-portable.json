    "license": {
        "identifier": "AGPL-3.0-or-later",
        "url": "https://github.com/ciderapp/Cider/blob/main/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/ciderapp/cider-releases/releases/download/v1.5.1-beta.440/Cider-Setup-1.5.1-beta.440.exe#/dl.7z",
            "hash": "sha512:b2ff682bce629e24acf4cb97a8930929016a01ca265957fe7dec727cbd68e14ef2c2c7d1d8ea6d2548dc28344f1964931f9cc7185506aa14dc269a25c112048a",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\" -Removal"
        }
    },
    "post_install": "Remove-Item \"$dir\\`$PLUGINSDIR\", \"$dir\\`$R0\" -Recurse -Force",
    "shortcuts": [
        [
            "Cider.exe",
