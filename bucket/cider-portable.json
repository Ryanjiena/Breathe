    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/ciderapp/cider-releases/releases/download/v1.5.1-beta.323/Cider-Setup-1.5.1-beta.323.exe#/dl.7z",
            "hash": "sha512:119c27a4de66bdadce5379176e4701bf352a8c79e051beb2d7110cf3f91b484e8e77a2f7b314ceaf413dc9a857edbe7d216069eb58da03c2ad1d1890dd76022d",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\" -Removal"
