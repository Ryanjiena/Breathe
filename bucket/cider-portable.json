    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/ciderapp/cider-releases/releases/download/v1.5.4-beta.41/Cider-Setup-1.5.4-beta.41.exe#/dl.7z",
            "hash": "sha512:37cb89241c017a7d8c8a0327278283cda174fa9a9311939a92b90f556b778d0bd09e27e26b9b24520d614dc2335ae276df28f4a063c47c45481740e9f64457be",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\" -Removal"
