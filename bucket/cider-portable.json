    "homepage": "https://cider.sh",
    "license": {
        "identifier": "AGPL-3.0-or-later",
        "url": "https://github.com/ciderapp/Cider/blob/main/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/ciderapp/cider-releases/releases/download/v1.5.4-beta.34/Cider-Setup-1.5.4-beta.34.exe#/dl.7z",
            "hash": "sha512:cd86368773ebcb9c29ef552a51c562bd8a2ce5fd02bb65652979ac70e48f79ce5169c3bf2f67b58c538f5019c2dfae320aed041d4f80aae141ebecf25756debd",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\" -Removal"
        }
    },
    "post_install": "Remove-Item \"$dir\\`$PLUGINSDIR\", \"$dir\\`$R0\" -Recurse -Force",
    "shortcuts": [
        [
            "Cider.exe",
            "Cider",
            "--user-data-dir=\"$dir\\User Data\""
        ]
    ],
    "persist": "User Data",
    "checkver": {
        "url": "https://api.github.com/repos/ciderapp/cider-releases/releases",
        "jsonpath": "$[0].tag_name",
        "regex": "v(.*)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://github.com/ciderapp/cider-releases/releases/download/v$version/Cider-Setup-$version.exe#/dl.7z",
                "hash": {
                    "url": "$baseurl/latest.yml",
                    "regex": "(?sm)$basename.*?sha512:\\s+$base64"
                }
            }
        }
    }
}
