    "homepage": "https://cider.sh",
    "license": {
        "identifier": "AGPL-3.0-or-later",
        "url": "https://github.com/ciderapp/Cider/blob/main/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/ciderapp/cider-releases/releases/download/v1.5.1-beta.353/Cider-Setup-1.5.1-beta.353.exe#/dl.7z",
            "hash": "sha512:bfa02ca359b3ed62b6ea9ddbb577d8819ff047465c8c5e0ea56913c2607c038c267416063c19a1e8e30dfee3426281d4eeb58429051cabc7af8950354698390f",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\" -Removal"
        }
    },
    "post_install": "Remove-Item \"$dir\\`$PLUGINSDIR\", \"$dir\\`$R0\" -Recurse -Force",
