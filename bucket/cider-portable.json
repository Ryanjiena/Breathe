{
    "version": "1.5.4-beta.49",
    "description": "A new cross-platform Apple Music experience based on Electron and Vue.js written from scratch with performance in mind.",
    "homepage": "https://cider.sh",
    "license": {
        "identifier": "AGPL-3.0-or-later",
        "url": "https://github.com/ciderapp/Cider/blob/main/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/ciderapp/cider-releases/releases/download/v1.5.4-beta.49/Cider-Setup-1.5.4-beta.49.exe#/dl.7z",
            "hash": "sha512:94e786be6a5536856b32dfeeb726852aec624fc00d838825f39a3c85fbb787d52233034dae7efeede2d1da7da1e5630a119246bf3f69c9db411278b7be246d1a",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\" -Removal"
        }
    },
    "post_install": "Remove-Item \"$dir\\`$PLUGINSDIR\", \"$dir\\`$R0\" -Recurse -Force",
    "shortcuts": [
        [
            "Cider.exe",
            "Cider",
            "--user-data-dir=\"$dir\\User Data\""
        ]
