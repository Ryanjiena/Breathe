    "homepage": "https://cider.sh",
    "license": {
        "identifier": "AGPL-3.0-or-later",
        "url": "https://github.com/ciderapp/Cider/blob/main/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/ciderapp/cider-releases/releases/download/v1.5.1-beta.327/Cider-Setup-1.5.1-beta.327.exe#/dl.7z",
            "hash": "sha512:7e5587a50a3669822d3060259c01fd200884b3cfeea8be2ebabfc2d744df93af4d7c013a8b94effc483353d0fd37f5b80e434f28d3973ad72d5fac4784f9cf6e",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\" -Removal"
        }
    },
    "post_install": "Remove-Item \"$dir\\`$PLUGINSDIR\", \"$dir\\`$R0\" -Recurse -Force",
    "shortcuts": [
        [
            "Cider.exe",
            "Cider",
            "--user-data-dir=\"$dir\\User Data\""
