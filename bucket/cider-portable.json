    "homepage": "https://cider.sh",
    "license": {
        "identifier": "AGPL-3.0-or-later",
        "url": "https://github.com/ciderapp/Cider/blob/main/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/ciderapp/cider-releases/releases/download/v1.5.6-beta.49/Cider-Setup-1.5.6-beta.49.exe#/dl.7z",
            "hash": "sha512:2dad5a50d6929ea9494092c2f1d30f93d07df20cc3869b7ce4e79c419728c17b1bcb5f3ab4f92ff95266d2ed85ad56f7da74bd3bd6081de1bfdd5091dcce1dfd",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\" -Removal"
        }
    },
    "post_install": "Remove-Item \"$dir\\`$PLUGINSDIR\", \"$dir\\`$R0\" -Recurse -Force",
    "shortcuts": [
        [
            "Cider.exe",
            "Cider",
            "--user-data-dir=\"$dir\\User Data\""
        ]
    ],
