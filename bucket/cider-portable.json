    "license": {
        "identifier": "AGPL-3.0-or-later",
        "url": "https://github.com/ciderapp/Cider/blob/main/LICENSE"
    },
    "architecture": {
        "64bit": {
            "url": "https://github.com/ciderapp/cider-releases/releases/download/v1.5.4-beta.61/Cider-Setup-1.5.4-beta.61.exe#/dl.7z",
            "hash": "sha512:1645225db3eb8434d3e7add20acdc05006e0772a91e5222b0385c01dfbfef1bf48a701e37b058b94474d7a4df5b937b4c98622ff9f3f45401d8655d1e8df147c",
            "pre_install": "Expand-7zipArchive \"$dir\\`$PLUGINSDIR\\app-64.7z\" \"$dir\" -Removal"
        }
    },
    "post_install": "Remove-Item \"$dir\\`$PLUGINSDIR\", \"$dir\\`$R0\" -Recurse -Force",
