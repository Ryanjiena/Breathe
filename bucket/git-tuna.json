{
    "version": "2.36.1",
    "description": "Distributed version control system(Download from TUNA mirror).",
    "homepage": "https://gitforwindows.org",
    "license": "GPL-2.0-only",
    "architecture": {
        "64bit": {
            "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/git-for-windows/git/LatestRelease/PortableGit-2.36.1-64-bit.7z.exe#/dl.7z",
            "hash": "af17a2803c5c6406b9b60dfef2d34f72f218975f9d78df21005a44f6e2f0caf9"
        },
        "32bit": {
            "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/git-for-windows/git/LatestRelease/PortableGit-2.36.1-32-bit.7z.exe#/dl.7z",
            "hash": "71ad967137a4da096f3e3406bd8a761f59c3a1edbf32e81e69e1f75efb9a44c4"
        }
    },
    "post_install": "git config --global credential.helper manager-core",
    "bin": [
        "cmd\\git.exe",
        "cmd\\gitk.exe",
        "cmd\\git-gui.exe",
        "usr\\bin\\tig.exe",
        "git-bash.exe"
    ],
    "shortcuts": [
        [
            "git-bash.exe",
            "Git Bash",
            "--cd-to-home"
        ],
        [
            "cmd\\git-gui.exe",
            "Git GUI"
        ]
    ],
    "env_set": {
        "GIT_INSTALL_ROOT": "$dir"
    },
    "checkver": {
        "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/git-for-windows/git/",
        "regex": "Git for Windows ([\\d.]+)"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/git-for-windows/git/LatestRelease/PortableGit-$version-64-bit.7z.exe#/dl.7z"
            },
            "32bit": {
                "url": "https://mirrors.tuna.tsinghua.edu.cn/github-release/git-for-windows/git/LatestRelease/PortableGit-$version-32-bit.7z.exe#/dl.7z"
            }
        },
        "hash": {
            "url": "https://github.com/git-for-windows/git/releases",
            "regex": "<td>$basename</td>\\s*<td>$sha256</td>"
        }
    }
}
