{
    "description": "Official production-ready open-source builds of OpenJDK 21",
    "homepage": "https://jdk.java.net/21",
    "version": "21-4-ea",
    "license": "GPL-2.0-only WITH Classpath-exception-2.0",
    "architecture": {
        "64bit": {
            "url": "https://download.java.net/java/early_access/jdk21/4/GPL/openjdk-21-ea+4_windows-x64_bin.zip",
            "hash": "9d343a4d8986f8291ed65dc1a3eb1ebe6c674ba0e969f003989020e9a8484108"
        }
    },
    "extract_dir": "jdk-21",
    "env_add_path": "bin",
    "env_set": {
        "JAVA_HOME": "$dir"
    },
    "checkver": {
        "url": "https://jdk.java.net/21",
        "re": "/(?<type>early_access|GA)/(?<path>jdk(?<major>[\\d.]+)(?:.*)?/(?<build>[\\d]+)(?:/GPL|/binaries)?)/(?<file>openjdk-(?<version>[\\d.]+)(?<ea>-ea)?(?:\\+[\\d]+)?_windows-x64_bin.(zip|tar.gz))",
        "replace": "${version}-${build}${ea}"
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "https://download.java.net/java/$matchType/$matchPath/$matchFile"
            }
        },
        "hash": {
            "url": "$url.sha256"
        },
        "extract_dir": "jdk-$matchVersion"
    }
}
