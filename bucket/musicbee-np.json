    "hash": "367a2672dcbe0531c2b1d2bf8ee2eabf96755a130f014ae0a8ea84ad59a451dc",
    "installer": {
        "script": [
            "Start-Process -Wait \"$dir\\MusicBeeSetup_3_4_Update2.exe\" -ArgumentList @('/S') -Verb RunAs | Out-Null"
