{
    "version": "1.3-dev_2023-03-19",
    "description": "Professional vector graphics editor",
    "homepage": "https://inkscape.org",
    "license": "GPL-3.0-or-later",
    "architecture": {
        "64bit": {
            "url": "https://gitlab.com/inkscape/inkscape/-/jobs/3961446850/artifacts/raw/build/inkscape-1.3-dev_2023-03-19_4af90830b-x64.7z",
            "hash": "267ff332a3bbac0e54c5c5c4823dc242384648c8d3ab959fff3286cdc3b59869"
        },
        "32bit": {
            "url": "https://gitlab.com/inkscape/inkscape/-/jobs/3961110885/artifacts/raw/build/inkscape-1.3-dev_2023-03-17_767a45dea-x86.7z",
            "hash": "5e2d8765bd1d513f4ab938dd77fde1e2fd8192ad75a84f96467fe4daaf69f6b7"
        }
    },
    "extract_dir": "inkscape",
    "bin": [
        "bin\\inkscape.com",
        "bin\\inkview.com"
    ],
    "shortcuts": [
        [
            "bin\\inkscape.exe",
            "Inkscape Dev"
        ]
    ],
    "checkver": {
        "script": [
            "$urls =  @()",
            "'x32', 'x64' | ForEach-Object {",
            "    $base_url = 'https://alpha.inkscape.org/snapshots/latest_master_'+ $_ + '.html'",
            "    $dl_page = Invoke-WebRequest $base_url -UseBasicParsing",
            "    $dl = $dl_page.links | Where-Object href -match '\\.7z$' | Select-Object -first 1 -expand href",
            "    $urls += $dl",
            "}",
            "Write-Output ('urls:\"' + $urls + '\"')"
        ],
        "regex": "urls:\"(?<win32bit>.+inkscape-[\\d._\\-dev]+_\\w+-.+)\\s(?<win64bit>.+inkscape-(?<version>[\\d._\\-dev]+)_\\w+-.+)\""
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchWin64bit"
            },
            "32bit": {
                "url": "$matchWin32bit"
            }
        }
    }
}
