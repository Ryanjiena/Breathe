{
    "version": "1.3-dev_2023-03-27",
    "description": "Professional vector graphics editor",
    "homepage": "https://inkscape.org",
    "license": "GPL-3.0-or-later",
    "architecture": {
        "64bit": {
            "url": "https://gitlab.com/inkscape/inkscape/-/jobs/4005135927/artifacts/raw/build/inkscape-1.3-dev_2023-03-27_2d13dde82-x64.7z",
            "hash": "918ba4d9038c111c49b33fdd57d72045740dc5862e0e1a6e9ac7de91725225c4"
        },
        "32bit": {
            "url": "https://gitlab.com/inkscape/inkscape/-/jobs/4002339722/artifacts/raw/build/inkscape-1.3-dev_2023-03-24_009e769f6-x86.7z",
            "hash": "67a2b2e0b4c1d7b3ed82a79c38b4bf7d578375e72e1c135ba84c1d101c45f663"
        }
    },
    "extract_dir": "inkscape",
    "bin": [
        "bin\\inkscape.com",
        "bin\\inkview.com"
    ],
    "shortcuts": [
        [
            "bin\\inkscape.exe",
            "Inkscape Dev"
        ]
    ],
    "checkver": {
        "script": [
            "$urls =  @()",
            "'x32', 'x64' | ForEach-Object {",
            "    $base_url = 'https://alpha.inkscape.org/snapshots/latest_master_'+ $_ + '.html'",
            "    $dl_page = Invoke-WebRequest $base_url -UseBasicParsing",
            "    $dl = $dl_page.links | Where-Object href -match '\\.7z$' | Select-Object -first 1 -expand href",
            "    $urls += $dl",
            "}",
            "Write-Output ('urls:\"' + $urls + '\"')"
        ],
        "regex": "urls:\"(?<win32bit>.+inkscape-[\\d._\\-dev]+_\\w+-.+)\\s(?<win64bit>.+inkscape-(?<version>[\\d._\\-dev]+)_\\w+-.+)\""
    },
    "autoupdate": {
        "architecture": {
            "64bit": {
                "url": "$matchWin64bit"
            },
            "32bit": {
                "url": "$matchWin32bit"
            }
        }
    }
}
